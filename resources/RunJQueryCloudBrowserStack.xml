<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">

<suite name="Data Table - Data Cloud" verbose="1">
       <test name="Run on Chrome browser">
           <parameter name="url" value="https://www.jqueryscript.net/demo/CRUD-Data-Grid-Plugin-jQuery-Quickgrid/"></parameter>
        <parameter name="browser" value="chrome"></parameter>
        <parameter name="osName" value="windows"></parameter>
        <parameter name="osVersion" value="11"></parameter>
        <parameter name="browserVersion" value="latest"></parameter>
        <classes>
            <class name="com.jquery.table.Level_33_CLOUD_BROWSERSTACK"/>
        </classes>
    </test>

    <test name="Run on Firefox browser">
        <parameter name="url" value="https://www.jqueryscript.net/demo/CRUD-Data-Grid-Plugin-jQuery-Quickgrid/"></parameter>
        <parameter name="browser" value="firefox"></parameter>
        <parameter name="osName" value="windows"></parameter>
        <parameter name="osVersion" value="11"></parameter>
        <parameter name="browserVersion" value="latest"></parameter>
        <classes>
            <class name="com.jquery.table.Level_33_CLOUD_BROWSERSTACK"/>
        </classes>
    </test>

    <test name="Run on Edge browser">
        <parameter name="url" value="https://www.jqueryscript.net/demo/CRUD-Data-Grid-Plugin-jQuery-Quickgrid/"></parameter>
        <parameter name="browser" value="edge"></parameter>
        <parameter name="osName" value="windows"></parameter>
        <parameter name="osVersion" value="10"></parameter>
        <parameter name="browserVersion" value="latest"></parameter>
        <classes>
            <class name="com.jquery.table.Level_33_CLOUD_BROWSERSTACK"/>
        </classes>
    </test>
</suite>