<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">

<suite name="Data Table - Data Cloud" verbose="1">
    <parameter name="url" value="https://www.jqueryscript.net/demo/CRUD-Data-Grid-Plugin-jQuery-Quickgrid/"></parameter>

    <test name="Run on Window 11 - Chrome browser">
        <parameter name="osName" value="Windows 11"></parameter>
        <parameter name="browser" value="chrome"></parameter>
        <parameter name="browserVersion" value="latest"></parameter>
        <classes>
            <class name="com.jquery.table.Level_34_CLOUD_SAUCELAB"/>
        </classes>
    </test>

    <test name="Run on Mac - Ventura browser">
        <parameter name="osName" value="macOS 13"></parameter>
        <parameter name="browser" value="safari"></parameter>
        <parameter name="browserVersion" value="latest"></parameter>
        <classes>
            <class name="com.jquery.table.Level_34_CLOUD_SAUCELAB"/>
        </classes>
    </test>

    <test name="Run on Window 10 - firefox browser">
        <parameter name="browser" value="firefox"></parameter>
        <parameter name="osName" value="Windows 10"></parameter>
        <parameter name="browserVersion" value="130"></parameter>
        <classes>
            <class name="com.jquery.table.Level_34_CLOUD_SAUCELAB"/>
        </classes>
    </test>
</suite>